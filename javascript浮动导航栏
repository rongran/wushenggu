//导航栏悬停事件

<script type="text/javascript"> 
window.onscroll=function(){
  var t = document.documentElement.scrollTop || document.body.scrollTop;
  if(t>90){
    $(".nav").css({"position":"fixed","z-index":9999,"top":0,"width":"100%"});
  }else{
    $(".nav").css("position","static");
  }
}
</script> 
